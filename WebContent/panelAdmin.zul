					</menubar>
			    </div>
				<div  width="100%" height="auto" sclass="estilo_tabbox">
					<div sclass="contenedor_botones" width="100%">
						<div  width="100%" height="auto">
							<div sclass="estilo_tabbox visible_peque" width="100%">
								<div>
									<button id="tabDisponibilidad" zclass="btn_opciones_config" label="Actualizar Disponibilidad" onClick="@command('Cambio',cambioInterfaz=self.id)" focus="@load(vmAdmin.visibleDisponibilidad)"></button>
									<button id="tabEtiqueta" zclass="btn_opciones_config" label="Etiquetas" onClick="@command('Cambio',cambioInterfaz=self.id)" focus="@load(vmAdmin.visibleEtiqueta)"></button>
									<button id="tabPaquete" zclass="btn_opciones_config" label="Paquetes" onClick="@command('Cambio',cambioInterfaz=self.id)" focus="@load(vmAdmin.visiblePaquetes)"></button>
									<button id="tabServicio" zclass="btn_opciones_config" label="Servicios" onClick="@command('Cambio',cambioInterfaz=self.id)" focus="@load(vmAdmin.visibleServicios)"></button>
									<button id="tabSubServicio" zclass="btn_opciones_config" label="Sub Servicios" onClick="@command('Cambio',cambioInterfaz=self.id)" focus="@load(vmAdmin.visibleSubServicios)"></button>
									<button id="tabImpuesto" zclass="btn_opciones_config" label="Impuestos" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="tabHotel" zclass="btn_opciones_config" label="Hoteles" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="tabDestino" zclass="btn_opciones_config" label="Destinos" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="tabReporteReserva" zclass="btn_opciones_config" label="Reporte Reservas" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
								</div>
								<div>
									<div visible="@bind(vmAdmin.visibleDisponibilidad)">
										<include src="UpdateDispo.zul"/>	
									</div>
									<div visible="@bind(vmAdmin.visibleEtiqueta)" >
										<include src="etiquetas.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visiblePaquetes)">
									   <include src="paquetes.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleServicios)">
