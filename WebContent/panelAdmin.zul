									</div>
									<div visible="@bind(vmAdmin.visibleDestinos)">
										<include src="destinos.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleReportReservas)">
										<include src="reporte_reservas.zul"/>
									</div>
								</div>
							</div>
							<div sclass="estilo_tabbox visible_grande"  visible="@bind(vmAdmin.visibleConfiguracion)" width="100%">
								<div>
									<button id="btnDisponibilidad" zclass="btn_opciones_config" label="Actualizar Disponibilidad" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnEtiquetas" zclass="btn_opciones_config" label="Etiquetas" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnPaquetes" zclass="btn_opciones_config" label="Paquetes" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnServicios" zclass="btn_opciones_config" label="Servicios"  onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnSubServicios" zclass="btn_opciones_config" label="Sub Servicios" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnImpuestos" zclass="btn_opciones_config" label="Impuestos" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnHoteles" zclass="btn_opciones_config" label="Hoteles"  onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnDestinos" zclass="btn_opciones_config" label="Destinos" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
									<button id="btnReporteReservas" zclass="btn_opciones_config" label="Reporte Reservas" onClick="@command('Cambio',cambioInterfaz=self.id)"></button>
								</div>
								<div>
									<div visible="@bind(vmAdmin.visibleDisponibilidad)">
										<include src="UpdateDispo.zul"/>	
									</div>
									<div visible="@bind(vmAdmin.visibleEtiqueta)">
										<include src="etiquetas.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visiblePaquetes)">
									   <include src="paquetes.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleServicios)">
									  <include src="servicios.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleSubServicios)">
										<include src="subServicios.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleImpuestos)">
										<include src="impuestos.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleHoteles)">
										<include src="hoteles.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleDestinos)">
										<include src="destinos.zul"/>
									</div>
									<div visible="@bind(vmAdmin.visibleReportReservas)">
										<include src="reporte_reservas.zul"/>
									</div>
